              "title": "wwDotNetBridge::LoadAssembly",
              "slug": "Utility-Classes/Class-wwDotnetBridge/wwDotNetBridgeLoadAssembly",
              "link": "Utility-Classes/Class-wwDotnetBridge/wwDotNetBridgeLoadAssembly.md",
              "displayType": "classmethod",
